{
  "buildCommand": "prisma generate && next build",
  "outputDirectory": ".next",
  "devCommand": "next dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/backup/auto",
      "schedule": "0 2 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/shop",
      "destination": "/catalog",
      "permanent": false
    },
    {
      "source": "/store",
      "destination": "/catalog",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/catalog(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
